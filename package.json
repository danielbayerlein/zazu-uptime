{
  "name": "zazu-uptime",
  "version": "1.1.1",
  "description": "Zazu plugin to show the system uptime",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "lint": "standard --verbose | snazzy",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielbayerlein/zazu-uptime.git"
  },
  "keywords": [
    "zazu",
    "uptime"
  ],
  "author": "Daniel Bayerlein",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danielbayerlein/zazu-uptime/issues"
  },
  "homepage": "https://github.com/danielbayerlein/zazu-uptime#readme",
  "devDependencies": {
    "husky": "^2.2.0",
    "jest": "^24.7.1",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1"
  },
  "jest": {
    "bail": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/*.js"
    ]
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  }
}
